#include <iostream>
include <string>
using namespace std;

int main()
{
  //string variables
  string day1 = "Monday";
  string day2 = "Tuesday";
  string day3 = "Wednesday";
  string day4 = "Thursday";
  string day5 = "Friday";
  string day6 = "Saturday";
  string day7 = "Sunday";
  
  cout << "String data type: " << endl;
  cout << day1 << endl;
  return 0;
}
